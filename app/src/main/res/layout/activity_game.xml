<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Game">

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphfinal"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphinput1"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:layout_marginTop="19dp"
        app:layout_constraintBottom_toTopOf="@+id/graphinput2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graphfinal" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphinput2"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        app:layout_constraintBottom_toTopOf="@+id/graphinput3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graphinput1" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/graphinput3"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/graphinput2" />


</android.support.constraint.ConstraintLayout>